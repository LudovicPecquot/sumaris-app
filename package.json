  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start.dev": "ng serve --host 0.0.0.0 --disable-host-check",
    "start.prod": "ng serve  --configuration production",
    "build": "ng build",
    "build.dev": "ng build --configuration development",
    "build.ci": "ng build --configuration ci",
    "build.prod": "ng build --configuration production && npm run build.i18n",
    "build.i18n": "node scripts/node/build-i18n.js",
    "android-sync": "npx jetifier && npx cap sync android",
    "android-start": "ionic capacitor run android -l --external",
    "android-build": "npx jetifier && ionic capacitor build android --no-open",
    "android-build.dev": "npx jetifier && ionic capacitor build android --no-open --configuration development",
    "android-build.prod": "npx jetifier && ionic capacitor build android --no-open --configuration production --release && npm run build.i18n",
    "android-list": "npx native-run android --list",
    "resources": "cordova-res ios && cordova-res android && node scripts/node/resources.js",
    "test": "ng test",
    "lint": "node --max-old-space-size=6096 node_modules/@angular/cli/bin/ng lint",
    "lint.fix": "npm run lint --fix"
  },
